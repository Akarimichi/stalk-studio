{
  "name": "stalk-studio",
  "version": "0.1.0",
  "description": "An inspection tool for OpenTracing-compatible traces",
  "homepage": "https://github.com/dgurkaynak/stalk-studio",
  "repository": "github:dgurkaynak/stalk-studio",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Deniz Gurkaynak",
    "email": "dgurkaynak@gmail.com",
    "url": "https://github.com/dgurkaynak"
  },
  "main": "main.js",
  "dependencies": {
    "node-int64": "^0.4.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "format": "prettier --write \"src/**/*.ts\"",
    "package": "electron-builder --dir",
    "distribute": "electron-builder",
    "start:electron": "node scripts/electron-wait-webpack-dev-server.js",
    "start:dev-server": "webpack-dev-server --port 9000",
    "start": "cross-env BROWSER=none nf start"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.6.5",
    "@mdi/svg": "^4.5.95",
    "@phosphor/widgets": "^1.9.3",
    "@types/chroma-js": "^1.4.3",
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.133",
    "@types/node": "12.7.12",
    "chance": "^1.1.3",
    "chroma-js": "^2.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "dexie": "^2.0.4",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "events": "^3.0.0",
    "foreman": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "fuse.js": "^3.4.6",
    "google-palette": "^1.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-inline-svg-plugin": "^1.2.4",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.18.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "noty": "^3.2.0-beta",
    "opentracing": "^0.14.4",
    "parse-duration": "^0.1.1",
    "prettier": "1.19.1",
    "raw-loader": "^3.1.0",
    "shortid": "^2.2.15",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "tabulator-tables": "^4.5.3",
    "tippy.js": "^5.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "uglifycss": "0.0.29",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "xstate": "^4.7.0-rc5"
  },
  "build": {
    "appId": "com.dgurkaynak.stalk-studio",
    "files": [
      "main.js",
      "build/**/*",
      "!build/**/*.js.map",
      "!build/report.html"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "type": "distribution",
      "fileAssociations": [
        {
          "ext": [
            "json",
            "JSON"
          ],
          "description": "JSON files",
          "role": "Viewer"
        }
      ]
    }
  }
}
